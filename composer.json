{
    "name": "dagryazev/wbwebapi",
    "description": "Wildberries WebAPI exploit",
    "keywords": ["wildberries", "web", "api"],
    "type": "library",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.0"
    },
    "scripts": {
        "tests": "phpunit -v -c ./tests/phpunit.xml tests"
    },
    "autoload": {
        "psr-4": {
            "Dakword\\WBWebAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dakword\\WBWebAPI\\Tests\\": "tests/"
        }
    }
}
